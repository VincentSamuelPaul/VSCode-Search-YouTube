{
    "name": "searchyoutube",
    "displayName": "search youtube videos",
    "description": "This Extension helps you to search YouTube videos directly from your favorite code editor.",
    "publisher": "VincentSamuelPaul",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.95.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:search.searchYoutube"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [{
            "command": "search.searchYoutube",
            "title": "Search YouTube"
        }]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.95.0",
        "@types/mocha": "^10.0.9",
        "@types/node": "20.x",
        "eslint": "^9.13.0",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1"
    }
}